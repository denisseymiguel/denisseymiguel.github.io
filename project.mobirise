          "_customHTML": "<section data-bs-version=\"5.1\" class=\"mbr-section\" group=\"Dentaura Preloader\" plugins=\"Dentaurapreloader\" once=\"Dentaura Preloader\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"color\" title=\"Background Color\" name=\"preBgcolor\" value=\"#ffffff\">\n        <select title=\"Preloader Color\" name=\"showLoaderborder\">\n            <option value=\"singleColor\" selected=\"\">Single</option>\n\t        <option value=\"multiColor\">Multi color</option>\n        </select>\n        <input type=\"color\" title=\"Preloader Color\" name=\"preLoadcolor\" value=\"#8a743d\" condition=\"showLoaderborder == 'singleColor' &amp;&amp; showLoaderborder !== 'multiColor'\">\n        <input type=\"color\" title=\"Preloader Color 1\" name=\"preLoadcolorone\" value=\"#f7ed4a\" condition=\"showLoaderborder == 'multiColor' &amp;&amp; showLoaderborder !== 'singleColor'\">\n        <input type=\"color\" title=\"Preloader Color 2\" name=\"preLoadcolortwo\" value=\"#ffffff\" condition=\"showLoaderborder == 'multiColor' &amp;&amp; showLoaderborder !== 'singleColor'\">\n        <input type=\"range\" inline=\"\" title=\"Image border round\" name=\"borderRadius\" min=\"0\" max=\"50\" step=\"1\" value=\"31\">\n        <input type=\"range\" inline=\"\" title=\"Preloader border round\" name=\"preloaderborderRadius\" min=\"0\" max=\"5\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Preloader animation Speed\" name=\"animSpeed\" min=\"1\" max=\"4\" step=\"1\" value=\"3\">\n        <input type=\"checkbox\" title=\"Enable Logo\" name=\"showLogo\" checked=\"\">\n        <select title=\"Logo Background\" name=\"showLoadercolor\">\n            <option value=\"transparentColor\">Transparent</option>\n\t        <option value=\"backgroundColor\" selected=\"\">Color</option>\n        </select>\n        <input type=\"color\" title=\"Logo Background Color\" name=\"logobgColor\" value=\"#ffffff\" condition=\"showLoadercolor == 'backgroundColor'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n    <!-- Dentaura Preloader -->\n        <div id=\"loading-preloader\">\n            <div id=\"loading-center-preloader\">\n                <div id=\"loading-center-absolute\">\n                    <div class=\"dentaura-preloader-cover\">\n                    <div class=\"dentaura-logo-cover\">\n<img src=\"@PROJECT_PATH@/assets/images/dm-initial-wedding-monogram-logo-vector-38259274-1-232x251.jpg\" class=\"dentaura-logo-preloader\" alt=\"\" mbr-if=\"showLogo\">\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    <!--End of Dentaura Preloader -->\n    \n</section>",
